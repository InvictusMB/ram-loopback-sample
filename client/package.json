{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "awilix": "^4.2.6",
    "date-fns": "^2.14.0",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "mobx-task": "^2.0.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "sanitize-html": "^1.23.0"
  },
  "scripts": {
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "serve": "react-scripts start",
    "prestart": "npm-run-all -p build:style generate-openapi",
    "start": "npm-run-all -p serve watch-resolver",
    "generate-openapi": "wait-on http://localhost:3000/api && node ./scripts/openapi/generate",
    "prebuild": "npm-run-all -p build:style generate-openapi",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch-resolver": "node ./scripts/watch-resolver"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.13-4.3.1",
    "@ram-stack/composition-root": "^1.0.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^25.2.2",
    "@types/lodash": "^4.14.151",
    "@types/mobx-task": "^2.0.1",
    "@types/node": "^14.0.1",
    "@types/qs": "^6.9.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/sanitize-html": "^1.23.0",
    "autoprefixer": "^9.8.0",
    "chokidar": "^3.4.0",
    "http-proxy-middleware": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.2"
  }
}
